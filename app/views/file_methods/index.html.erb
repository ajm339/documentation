<h1>Listing file_methods</h1>

<table>
  <thead>
    <tr>
      <th>Created by</th>
      <th>Name</th>
      <th>Description</th>
      <th>Input</th>
      <th>Output</th>
      <th>File instance</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @file_methods.each do |file_method| %>
      <tr>
        <td><%= file_method.created_by %></td>
        <td><%= file_method.name %></td>
        <td><%= file_method.description %></td>
        <td><%= file_method.input %></td>
        <td><%= file_method.output %></td>
        <td><%= file_method.file_instance %></td>
        <td><%= link_to 'Show', file_method %></td>
        <td><%= link_to 'Edit', edit_file_method_path(file_method) %></td>
        <td><%= link_to 'Destroy', file_method, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New File method', new_file_method_path %>
